{
  "name": "crypto-dashboard",
  "description": "Crypto Dashboard API",
  "version": "0.0.0",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Matt",
    "email": "matt@ideajunction.uk"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run coverage",
    "lint": "standard --fix",
    "start": "node src/",
    "clean": "shx rm -rf test/data/",
    "coverage": "npm run clean && nyc --reporter=text-summary mocha --timeout 999999 test/ --recursive --exit",
    "mocha": "npm run clean && NODE_ENV=test mocha test/ --recursive --exit"
  },
  "standard": {
    "envs": [
      "mocha"
    ]
  },
  "dependencies": {
    "@feathersjs/configuration": "^1.0.2",
    "@feathersjs/errors": "^3.3.0",
    "@feathersjs/express": "^1.2.0",
    "@feathersjs/feathers": "^3.1.3",
    "@feathersjs/socketio": "^3.2.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "feathers-hooks-common": "^4.10.0",
    "feathers-nedb": "^2.7.0",
    "helmet": "^3.12.0",
    "nedb": "^1.8.0",
    "serve-favicon": "^2.4.5",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "eslint": "^4.19.0",
    "mocha": "^5.0.4",
    "nyc": "^11.6.0",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "shx": "^0.2.2",
    "standard": "^11.0.1"
  }
}
